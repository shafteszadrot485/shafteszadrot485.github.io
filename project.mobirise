        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 50px",
                "@media (max-width: 1440px)": {
                  "padding": "0 40px"
                },
                "@media (max-width: 992px)": {
                  "padding": "0 20px"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 20px"
              }
            },
            ".title-wrapper": {
              "margin-bottom": "150px",
              "@media (max-width: 992px)": {
                "margin-bottom": "40px"
              },
              ".title-wrap": {
                "display": "inline-flex",
                "@media (max-width: 992px)": {
                  "display": "block"
                },
                "&:hover, &:focus": {
                  ".mbr-section-title": {
                    "color": "@icon"
                  }
                },
                ".mbr-section-title": {
                  "margin-bottom": "0",
                  "transition": "all 0.3s ease-in-out"
                },
                ".mbr-iconfont": {
                  "display": "inline-flex",
                  "color": "@icon",
                  "font-size": "60px",
                  "padding-top": "1.5rem",
                  "margin-left": "10px"
                }
              }
            },
            ".item": {
              "margin-bottom": "120px",
              "@media (max-width: 992px)": {
                "margin-bottom": "40px"
              },
              ".item-wrapper": {
                ".card-box": {
                  ".item-title": {
                    "margin-bottom": "0"
                  },
                  ".item-desc": {
                    "margin-bottom": "0",
                    "opacity": ".5"
                  },
                  ".list": {
                    "margin": "0",
                    "padding": "0",
                    "list-style-type": "none",
                    ".item-wrap": {
                      "margin-bottom": "0",
                      "transition": "all 0.3s ease-in-out",
                      "cursor": "pointer",
                      "text-decoration": "underline",
                      "text-decoration-color": "transparent",
                      "&:hover, &:focus": {
                        "text-decoration-color": "@primaryColor"
                      }
                    }
                  }
                }
              }
            },
            ".border-wrap": {
              "background-color": "@border",
              "height": "1px",
              "width": "100%"
            },
            ".copy-wrapper": {
              "display": "flex",
              "justify-content": "space-between",
              "align-items": "center",
              "padding": "40px 0",
              "& when not (@showButtons)": {
                "display": "block"
              },
              "& when not (@showCopy)": {
                "display": "block"
              },
              "@media (max-width: 992px)": {
                "padding": "30px 0",
                "flex-direction": "column-reverse"
              },
              ".mbr-copy": {
                "margin-bottom": "0",
                "margin-right": "20px",
                "@media (max-width: 992px)": {
                  "width": "100%",
                  "margin-right": "0"
                }
              },
              ".mbr-section-btn": {
                "@media (max-width: 992px)": {
                  "width": "100%",
                  "margin-bottom": "20px"
                },
                ".btn": {
                  "text-decoration": "none",
                  "position": "relative",
                  "&::before": {
                    "content": "'*'",
                    "position": "absolute",
                    "top": "38%",
                    "right": "-14px",
                    "color": "@marker",
                    "font-size": "20px"
                  },
                  "&:last-child": {
                    "&::before": {
                      "display": "none"
                    }
                  }
                }
              }
            },
            ".item-menu-overlay": {
              ".btn-wrapper": {
                "margin-top": "-6%"
              }
            },
            ".mbr-section-title": {
              "color": "#0f1d07"
            },
            ".item-title": {
              "color": "#0f1d07"
            },
            ".item-desc": {
              "color": "#0f1d07"
            },
            ".list": {
              "color": "#0f1d07"
            },
            ".mbr-copy": {
              "color": "#0f1d07"
            }
